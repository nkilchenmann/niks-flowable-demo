				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"\r",
							"var processInstanceId = jsonData.processInstanceId;\r",
							"\r",
							"pm.environment.set(\"processInstanceId\",processInstanceId);\r",
							"\r",
							"console.log(\"processInstanceId: \" + processInstanceId);"
